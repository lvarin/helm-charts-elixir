{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "host_name": {
            "type": "string",
            "description": "Host name"
        },
        "storageClass": {
            "description": "Configurable storage class",
            "type": ["string", "null"]
        },
        "tesk": {
            "properties": {
              "image": {
                "description": "TESK image",
                "type": "string"
              },
              "debug": {
                "description": "Activate debug log",
                "type": "boolean"
              }
            }
        }
    }
}
